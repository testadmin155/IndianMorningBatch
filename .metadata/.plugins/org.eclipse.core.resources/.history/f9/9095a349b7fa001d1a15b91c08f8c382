package FirstPackage;

public class DemoMethod {
	
	public void getData() 
	{
		System.out.println("This to get Data");
	}
	
	
	public void Paymentdetails()
	{
		System.out.println("Method to get the payment details");
	}

	public static void main(String[] args) {
		
		DemoMethod x =new DemoMethod();
		Methods y = new Methods();
		
		
		x.getData();
		x.Paymentdetails();
		
		y.login("Anees", "Password");
		
		
	}

}
