package FirstPackage;

public class Constructor {
	
	//constructorsshould the same name as class
	// constructor should not have void(return type)
	// constructor will be executed automatically when the object is create

	public Constructor()
	{
		System.out.println("I am a constructor1");
	}
	
	public Constructor(int x)
	{
		System.out.println("I am a constructor2");
	}
	
	public Constructor(String a, int x)
	{
		System.out.println("I am a constructor3");
	}
	
	public Constructor(String b, String a)
	{
		System.out.println("I am a constructor4");
	}

	public void getData()
	{
		System.out.println("this is  a method");
	}
	
	public static void main(String[] args) {
		
		Constructor obj =new Constructor("testing");
		//obj.getData();
		

	}

}
